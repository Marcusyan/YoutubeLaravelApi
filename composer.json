{
    "name": "marcusyan/youtube-laravel-api",
    "description": "It is a wrapper over Youtube Api v3 which simplifies functionalities the laravel way.",
    "keywords": ["alchemyguy", "YoutubeLaravelApi", "Youtube LiveStream", "php", "Youtube Backup", "Youtube Video", "Youtube Api V3", "laravel", "Oauth", "google api", "Youtube Channel"],
    "homepage": "https://github.com/alchemyguy/YoutubeLaravelApi",
    "require": {
        "php": "~5.6|~7.0",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "alchemyguy\\YoutubeLaravelApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "alchemyguy\\YoutubeLaravelApi\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style":
            "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style":
            "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
